prefix=/usr
exec_prefix=${prefix}

XIM=ibus
XIM_PROGRAM="${exec_prefix}/bin/ibus-daemon"
ICON=ibus
XIM_ARGS="--xim"
PREFERENCE_PROGRAM="${exec_prefix}/bin/ibus-setup"
SHORT_DESC="IBus"
GTK_IM_MODULE=ibus

if test -f /usr/lib64/qt4/plugins/inputmethods/libqtim-ibus.so || \
   test -f /usr/lib/qt4/plugins/inputmethods/libqtim-ibus.so;
then
    QT_IM_MODULE=ibus
else
    QT_IM_MODULE=xim
fi

